#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,n,flag;
	cin>>t;
	for(int i=0;i<t;i++)
	{
	    flag=0;
	    cin>>n;
	    int k=n/2;
	    vector<int>arr(n);
	    for(int j=0;j<n;j++)
	    cin>>arr[j];
	    sort(arr.begin(),arr.end());
	    if(n%2==0&&n!=2)
	    {
	        n--;
	    }
	    
	    if(n==2)
	    {
	        if(arr[0]==arr[1])
	        cout<<arr[0]<<endl;
	        else
	        cout<<"NO Majority Element"<<endl;
	        continue;
	    }
	    for(int j=0;j<=n/2;j++)
	    {
	        if(arr[j]==arr[j+k])
	        {
	            flag=1;
	            cout<<arr[j]<<endl;
	            break;
	        }
	    }
	    if(flag==0)
	    cout<<"NO Majority Element"<<endl;
	}
	return 0;
}
