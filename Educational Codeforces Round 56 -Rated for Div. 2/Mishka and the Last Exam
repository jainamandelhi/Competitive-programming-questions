#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long n;
    cin>>n;
    long long arr[n/2];
    for(long long i=0;i<n/2;i++)
        cin>>arr[i];
    long long ans[n];
    for(long long i=0;i<n/2;i++)
    {
        if(i==0)
        {
            ans[0]=0;
            ans[n-1]=arr[i];
            continue;
        }
            if(arr[i]-ans[i-1]>ans[n-i])
            {
                ans[n-i-1]=ans[n-i];
                ans[i]=arr[i]-ans[n-i-1];
                continue;
            }
            ans[i]=ans[i-1];
            ans[n-i-1]=arr[i]-ans[i];
    }
    for(long long i=0;i<n;i++)
        cout<<ans[i]<<" ";
    return 0;
}
